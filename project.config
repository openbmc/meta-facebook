[access]
	inheritFrom = openbmc-all-repos
[access "refs/heads/*"]
	create = group github/openbmc/meta-facebook-maintainers
	push = +force group github/openbmc/meta-facebook-maintainers
	abandon = group github/openbmc/meta-facebook-maintainers
	forgeCommitter = group github/openbmc/meta-facebook-maintainers
	label-Code-Review = -2..+2 group github/openbmc/meta-facebook-maintainers
	label-Ok-To-Test = +0..+1 group github/openbmc/meta-facebook-maintainers
	label-Verified = -1..+1 group github/openbmc/meta-facebook-maintainers
	rebase = group github/openbmc/meta-facebook-maintainers
	submit = group github/openbmc/meta-facebook-maintainers
	editTopicName = group github/openbmc/meta-facebook-maintainers
[access "refs/tags/*"]
	push = +force group github/openbmc/meta-facebook-maintainers
	createTag = group github/openbmc/meta-facebook-maintainers
	createSignedTag = group github/openbmc/meta-facebook-maintainers
[submit]
	action = rebase if necessary
